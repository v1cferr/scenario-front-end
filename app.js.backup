// Fun√ß√µes de Automa√ß√£o das Lumin√°rias
async function toggleLuminaria(id) {
    console.log(`üîÑ TOGGLE INICIADO: Lumin√°ria ${id}`);
    console.log('üìä Estado atual das lumin√°rias:', luminariaStates);
    
    try {
        // Atualiza√ß√£o otimista da UI - atualizar imediatamente para responsividade
        const currentState = luminariaStates[id] || false;
        const newState = !currentState;
        console.log(`üîÑ Toggle otimista: Lumin√°ria ${id} ${currentState} -> ${newState}`);
        
        // Atualizar estado local e UI imediatamente
        luminariaStates[id] = newState;
        console.log('üìä Estado ap√≥s atualiza√ß√£o local:', luminariaStates);
        
        console.log('üé® Chamando updateLuminariaStateUI...');
        updateLuminariaStateUI(id, newState);
        
        console.log('üåê Fazendo requisi√ß√£o para API...');
        const response = await apiRequest(`/luminaires/automation/${id}/toggle`, { 
            method: 'POST' 
        });
        
        console.log(`‚úÖ Toggle confirmado via API: Lumin√°ria ${id}:`, response);
        
        // O SSE vai confirmar o estado real do servidor
        // Se houver diferen√ßa, o SSE ir√° corrigir automaticamente
        
    } catch (error) {
        console.error('‚ùå Erro ao alternar lumin√°ria:', error);
        
        // Reverter mudan√ßa otimista em caso de erro
        const revertState = !luminariaStates[id];
        console.log(`üîÑ Revertendo para estado anterior: ${revertState}`);
        luminariaStates[id] = revertState;
        updateLuminariaStateUI(id, revertState);
        
        showNotification('Erro ao controlar lumin√°ria: ' + error.message, 'error');
    }
}

