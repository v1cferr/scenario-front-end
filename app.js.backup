}

// Conex√£o SSE para atualiza√ß√µes em tempo real usando fetch + ReadableStream
function connectToSSE() {
    // Fechar conex√£o anterior se existir
    if (sseEventSource) {
        if (sseEventSource.abort) {
            sseEventSource.abort();
        }
        sseEventSource = null;
    }
    
    // Verificar se temos token de autentica√ß√£o
    if (!authToken) {
        console.log('‚ùå Token n√£o dispon√≠vel para SSE');
        return;
    }
    
    console.log('üîå Conectando ao SSE com Authorization header...');
    
    // Log de debug adicional
    console.log('üîß Debug SSE: Token dispon√≠vel?', !!authToken);
    console.log('üîß Debug SSE: User conectado?', !!currentUser);
    console.log('üîß Debug SSE: URL:', `${API_BASE_URL}/luminaires/automation/events`);
    
    // Usar fetch com ReadableStream para suportar headers customizados
    const sseUrl = `${API_BASE_URL}/luminaires/automation/events`;
    const controller = new AbortController();
    sseEventSource = controller; // Armazenar o controller para poder cancelar
    
    fetch(sseUrl, {
        method: 'GET',
        headers: {
            'Authorization': `Bearer ${authToken}`,
            'Accept': 'text/event-stream',
            'Cache-Control': 'no-cache'
        },
        signal: controller.signal
    })
    .then(response => {
        if (!response.ok) {
            throw new Error(`HTTP ${response.status}: ${response.statusText}`);
        }
        
        console.log('‚úÖ Conex√£o SSE estabelecida');
        console.log('üîß Debug SSE: Response headers:', Object.fromEntries(response.headers.entries()));
        showNotification('üîå Automa√ß√£o conectada! Estado em tempo real ativo.', 'success');
        updateSSEStatus(true);
        
        const reader = response.body.getReader();
        const decoder = new TextDecoder();
        
        // Fun√ß√£o para processar o stream
        function processStream() {
            return reader.read().then(({ done, value }) => {
                if (done) {
                    console.log('üîö Stream SSE terminado - tentando reconectar...');
                    // Reconectar ap√≥s stream terminar
                    setTimeout(() => {
                        if (authToken && currentUser) {
                            console.log('üîÑ Reconectando SSE ap√≥s stream terminar...');
                            connectToSSE();
                        }
                    }, 2000);
                    return;
                }
                
                // Decodificar dados recebidos
                const chunk = decoder.decode(value, { stream: true });
                console.log('üì¶ Chunk SSE recebido:', chunk);
                const lines = chunk.split('\n');
                
                for (let line of lines) {
                    if (line.startsWith('event:')) {
                        const eventType = line.substring(6).trim();
                        continue;
                    }
                    
                    if (line.startsWith('data:')) {
                        const eventData = line.substring(5).trim();
                        
                        try {
                            // Processar dados baseado no tipo de evento
                            if (eventData) {
                                const data = JSON.parse(eventData);
                                
                                // Detectar tipo de evento baseado na estrutura dos dados
                                if (data.allStates) {
                                    // Evento initial_state
                                    console.log('ÔøΩ Estado inicial das lumin√°rias recebido:', eventData);
                                    luminariaStates = data.allStates;
                                    updateLuminariaStatesUI();
                                } else if (data.luminariaId !== undefined && data.isOn !== undefined) {
                                    // Evento state_change
                                    console.log('üí° Mudan√ßa de estado recebida:', eventData);
                                    luminariaStates[data.luminariaId] = data.isOn;
                                    updateLuminariaStateUI(data.luminariaId, data.isOn);
                                    showNotification(
                                        `Lumin√°ria ${data.luminariaId} ${data.isOn ? 'ligada' : 'desligada'}`, 
                                        'info'
                                    );
                                }
                            }
                        } catch (error) {
                            console.error('Erro processando dados SSE:', error);
                        }
                    }
                }
                
                // Continuar lendo o stream
                return processStream();
            });
        }
        
        // Iniciar processamento do stream
        return processStream();
    })
    .catch(error => {
        if (error.name === 'AbortError') {
            console.log('üîå Conex√£o SSE cancelada');
            return;
        }
        
        console.error('‚ùå Erro na conex√£o SSE:', error);
        showNotification('Conex√£o com servidor perdida. Tentando reconectar...', 'warning');
        updateSSEStatus(false);
        
        // Tentar reconectar ap√≥s 5 segundos
        setTimeout(() => {
            if (authToken && currentUser) {
                console.log('üîÑ Tentando reconectar SSE...');
                connectToSSE();
            }
        }, 5000);
    });
}

// Fun√ß√£o para processar eventos SSE
function processSSEEvent(event) {
    if (!event.data) return;
    
    try {
        const data = JSON.parse(event.data);
        
        // Log do evento recebido para debug
        console.log(`üì° Evento SSE recebido [${event.type || 'unknown'}]:`, data);
        
        // Processar diferentes tipos de eventos
        if (event.type === 'heartbeat' || data.type === 'heartbeat') {
            console.log('üíì Heartbeat recebido - conex√£o ativa');
            return;
        }
        
        if (data.allStates) {
            // Evento initial_state
            console.log('üè† Estado inicial das lumin√°rias recebido');
            luminariaStates = data.allStates;
            updateLuminariaStatesUI();
        } else if (data.luminariaId !== undefined && data.isOn !== undefined) {
            // Evento state_change
            const currentLocalState = luminariaStates[data.luminariaId];
            console.log(`üí° SSE: Lumin√°ria ${data.luminariaId} ${data.isOn ? 'LIGADA' : 'DESLIGADA'} (local: ${currentLocalState})`);
            
            // Sempre atualizar o estado local com o que vem do servidor (fonte da verdade)
            luminariaStates[data.luminariaId] = data.isOn;
            
            // Atualizar UI apenas se o estado mudou realmente
            if (currentLocalState !== data.isOn) {
                updateLuminariaStateUI(data.luminariaId, data.isOn);
                console.log(`üîÑ UI atualizada via SSE: Lumin√°ria ${data.luminariaId} -> ${data.isOn}`);
            } else {
                console.log(`‚úÖ SSE confirmou estado j√° atualizado: Lumin√°ria ${data.luminariaId} = ${data.isOn}`);
            }
            
            // Mostrar notifica√ß√£o apenas se n√£o veio de uma a√ß√£o local recente
            if (!document.hidden) {
                showNotification(
                    `Lumin√°ria ${data.luminariaId} ${data.isOn ? 'ligada' : 'desligada'}`, 
                    'info'
                );
            }
        }
    } catch (error) {
        console.error('‚ùå Erro processando evento SSE:', error, 'Dados:', event.data);
    }
}

// Atualizar UI de uma lumin√°ria espec√≠fica
function updateLuminariaStateUI(luminariaId, isOn) {
    console.log(`üé® Atualizando UI: Lumin√°ria ${luminariaId} -> ${isOn ? 'LIGADA' : 'DESLIGADA'}`);
    
    const lumCard = document.querySelector(`[data-luminaria-id="${luminariaId}"]`);
    if (!lumCard) {
        console.error(`‚ùå Card da lumin√°ria ${luminariaId} n√£o encontrado na UI`);
        console.log('üîç Cards dispon√≠veis:', document.querySelectorAll('[data-luminaria-id]').length);
        return;
    }
    
    console.log(`‚úÖ Card encontrado para lumin√°ria ${luminariaId}`);
    
    const statusIndicator = lumCard.querySelector('.status-indicator');
    const toggleBtn = lumCard.querySelector('.toggle-btn');
    
    if (statusIndicator) {
        statusIndicator.className = `status-indicator ${isOn ? 'on' : 'off'}`;
        statusIndicator.innerHTML = `<i class="fas fa-lightbulb"></i> ${isOn ? 'Ligada' : 'Desligada'}`;
        console.log(`‚úÖ Status indicator atualizado: ${isOn ? 'Ligada' : 'Desligada'}`);
    } else {
        console.error(`‚ùå Status indicator n√£o encontrado para lumin√°ria ${luminariaId}`);
    }
    
    if (toggleBtn) {
        toggleBtn.className = `btn toggle-btn ${isOn ? 'btn-danger' : 'btn-success'}`;
        toggleBtn.innerHTML = `<i class="fas fa-power-off"></i> ${isOn ? 'Desligar' : 'Ligar'}`;
        console.log(`‚úÖ Toggle button atualizado: ${isOn ? 'Desligar' : 'Ligar'}`);
    } else {
        console.error(`‚ùå Toggle button n√£o encontrado para lumin√°ria ${luminariaId}`);
    }
    
    // For√ßar re-render se necess√°rio
    lumCard.style.opacity = '0.8';
    setTimeout(() => {
        lumCard.style.opacity = '1';
    }, 100);
}

// Atualizar UI de todas as lumin√°rias
function updateLuminariaStatesUI() {
    console.log('üîÑ Atualizando UI de todas as lumin√°rias:', luminariaStates);
    for (const [luminariaId, isOn] of Object.entries(luminariaStates)) {
        updateLuminariaStateUI(parseInt(luminariaId), isOn);
    }
}

